{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 250,
      "unique": false
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "isArticle": {
      "type": "boolean",
      "default": false
    },
    "presentation_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "required": false,
      "component": "shared.seo"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "url": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
